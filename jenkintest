pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'https://github.com/Navyavennam/project.git'
                echo 'https://github.com/Shruthi/project.git'
                echo 'https://github.com/Karishma/project.git'
                echo 'https://github.com/Mahesh/project.git'
            }
        }
        stage('Build') {
            steps {
                echo 'javac Sample.java'
                echo 'run build'
      
            }
        }
        stage('Test') {
            steps {
                echo 'java Sample'
                echo 'run test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying application...'
                echo "Deployment finished successfully." 
                
            }
        }
    }
    }
